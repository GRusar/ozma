var base_date,
	base_Duration,
	base_id,
	number_event=0,
	base_EntryText;
@Each
	base_date="@Current.Value";
	base_Duration="@Current.Duration";
	base_EntryText="@Current.EntryText";
	base_id=@Current.Id;
	if (number_event==num_ev)
	old(base_date, base_Duration, base_id, base_EntryText);
	number_event++;
@EndEach

