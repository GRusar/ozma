<div class="mytitle" onclick>
    <ul class="menu">
        <li class="submenu">
            <a class="bl" href="../nav" style="background:@Model.BackColor">Главное меню</a>
        </li>
        @Each.Menus
            <li class="submenu">
                <a class="bl" style="background:@Model.BackColor">@Current.Name</a>
                <ul class="submenu">
                    @Partial['MenubarSubmenu', Current.Items]
                </ul>
            </li>
        @EndEach
    </ul>
</div>