<div class="mytitle">
    <ul class="menu">
        @Each
            <li>
                <a class="bl" href="@Current.Link" style="background:@Current.Color">@Current.Name</a>
                <ul class="submenu" style="background:@Color">
                    @Partial['MenubarSubmenu', Current.Sub]
                </ul>
            </li>
        @EndEach
    </ul>
</div>