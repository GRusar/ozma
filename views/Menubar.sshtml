<div class="mytitle">
    <ul class="menu">
        <a class="bl" href="@Ent.Link" style="background:@Ent.Color">@Ent.Name</a>
        @Each.Lists
            <li>
                <a class="bl" style="background:@Current.Color">@Current.Name</a>
                <ul class="submenu" style="background:@Color">
                    @Partial['MenubarSubmenu', Current.Sub]
                </ul>
            </li>
        @EndEach
    </ul>
</div>