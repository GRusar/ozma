{$ADDRESS} {
	handle_path /api/* {
		reverse_proxy {$OZMA_DB_HOSTPORT}
	}

	handle /auth/* {
		reverse_proxy {$KEYCLOAK_HOSTPORT}
	}

	handle /report-generator/* {
		reverse_proxy {$OZMA_REPORT_GENERATOR_HOSTPORT}
	}

	handle {
		root /usr/share/caddy
		file_server
		try_files {path} /index.html
	}
}
